{
  "manifest_version": 2,
  "name": "Video Database Manager",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "storage",
    {"mediaGalleries": ["read", "allAutoDetected"]},
    "fileSystem", "webview"
  ]
}
